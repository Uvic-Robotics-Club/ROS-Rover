<launch>
  <!-- Start the joystick node -->

  <!-- Start the joy_to_motor node -->
  <!-- <node pkg="uvic_rover" type="front_left_right_nopid.py" name="joy_to_frontworm" output="screen" /> -->
  <node pkg="uvic_rover" type="phidget_simple_drive.py" name="joy_to_motor" output="screen" />

  <node pkg="uvic_rover" type="front_left_right_nopid.py" name="joyasdds_to_phidget" output="screen" />


  <node pkg="rosserial_python" type="serial_node.py" name="serial_node" output="screen">
    <param name="port" value="/dev/ttyACM0"/> 
    <param name="baud" value="57600"/> 
  </node>
</launch>
